            <component
              refId="Package\STAGE DATA TO DW DATA LOAD\INSERT &amp; UPDATE CHANGES TO DW TABLE"
              componentClassID="Microsoft.OLEDBCommand"
              contactInfo="OLE DB Command;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;2"
              description="Runs an SQL statement for each row in a data flow. For example, call a 'new employee setup' stored procedure for each row in the 'new employees' table. Note: running an SQL statement for each row of a large data flow may take a long time."
              name="INSERT &amp; UPDATE CHANGES TO DW TABLE"
              usesDispositions="true"
              validateExternalMetadata="False"
              version="2">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--------------------------------------------------------------------------------
-- SET VARIABLES
--------------------------------------------------------------------------------
declare @ProductSubcategoryKey as int = ?
declare @ProductCategoryKey as int = ?
declare @EnglishProductSubcategoryName as nvarchar(255) = ?
declare @NewGermanProductSubcategoryName as nvarchar(255) = ?

--------------------------------------------------------------------------------
-- INSERT WHERE THE KEY ROW DOES NOT EXIST
--------------------------------------------------------------------------------
;WITH CTE AS
(
SELECT 
    @ProductSubcategoryKey as [ProductSubcategoryKey],
    @ProductCategoryKey  as [ProductCategoryKey],
    @EnglishProductSubcategoryName  as [EnglishProductSubcategoryName],
    @NewGermanProductSubcategoryName as [NewGermanProductSubcategoryName]
)
INSERT INTO [dbo].[DimProductSubcategory]
SELECT 
    i.[ProductSubcategoryKey],
    i.[ProductCategoryKey],
    i.[EnglishProductSubcategoryName],
    i.[NewGermanProductSubcategoryName]
FROM CTE as i
WHERE NOT EXISTS
(
    SELECT 1
    FROM [dbo].[DimProductSubcategory] as e
    WHERE e.[ProductSubcategoryKey] = i.[ProductSubcategoryKey]
)

--------------------------------------------------------------------------------
-- UPDATE WHERE THE KEY ROW DOES EXIST
--------------------------------------------------------------------------------
UPDATE [dbo].[DimProductSubcategory]
SET 
[EnglishProductSubcategoryName] = @EnglishProductSubcategoryName,
[GermanProductSubcategoryName] = @NewGermanProductSubcategoryName
WHERE [ProductSubcategoryKey] = @ProductSubcategoryKey
AND 
(
    [GermanProductSubcategoryName] &lt;&gt; @NewGermanProductSubcategoryName
    OR [EnglishProductSubcategoryName] &lt;&gt;  @EnglishProductSubcategoryName
    OR [GermanProductSubcategoryName] is NULL
)


